      "operatingsystemrelease": [
        "9"
      ]
    },
    {
      "operatingsystem": "SLES"
    },
    {
      "operatingsystem": "Solaris"
    },
    {
      "operatingsystem": "AIX"
    },
    {
      "operatingsystem": "FreeBSD"
    },
    {
      "operatingsystem": "DragonFly"
    },
    {
      "operatingsystem": "NetBSD"
    },
    {
      "operatingsystem": "Archlinux"
